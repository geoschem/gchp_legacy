if( $plot == zquad_V_Q )         $grads $batch -l -c "run $GEOSUTIL/plots/quad/zquad -EXPID $expid -EXPORT1     V:DYN -EXPORT2   Q:MOIST $INFO "
if( $plot == zquad_OMEGA_T )     $grads $batch -l -c "run $GEOSUTIL/plots/quad/zquad -EXPID $expid -EXPORT1 OMEGA:DYN -EXPORT2     T:DYN $INFO "
if( $plot == zquad_OMEGA_Q )     $grads $batch -l -c "run $GEOSUTIL/plots/quad/zquad -EXPID $expid -EXPORT1 OMEGA:DYN -EXPORT2   Q:MOIST $INFO "
if( $plot == zquad_KE_KE )       $grads $batch -l -c "run $GEOSUTIL/plots/quad/zquad -EXPID $expid -EXPORT1    KE:DYN -EXPORT2    KE:DYN $INFO "


