if( $plot == zquad_U_U )         $grads $batch -l -c "run $GEOSUTIL/plots/quad/zquad -EXPID $expid -EXPORT1     U:DYN -EXPORT2     U:DYN $INFO "
if( $plot == zquad_V_V )         $grads $batch -l -c "run $GEOSUTIL/plots/quad/zquad -EXPID $expid -EXPORT1     V:DYN -EXPORT2     V:DYN $INFO "
